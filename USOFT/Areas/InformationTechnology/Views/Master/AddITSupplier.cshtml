
@{
    ViewBag.Title = "AddITSupplier";
}
@using (Html.BeginForm(FormMethod.Post))
{
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">ADD IT SUPPLIER</h1>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Add New IT Supplier
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-8">

                            <form role="form">
                                <div class="form-group">
                                    @Html.Label("Supplier Code:", new { @class = "col-md-4 control-label" })
                                    <div class="col-lg-5">
                                        @*@Html.DropDownList("ParamField", (IEnumerable<SelectListItem>)ViewBag.menu, "All", new { @class = "form-control" })*@
                                        @Html.Label("[Generated By System]",  new { @class = "col-lg-10 control-label" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    @Html.Label("Supplier Name", new { @class = "col-md-4 control-label" })
                                    <div class="col-lg-5">
                                        @*@Html.DropDownList("ParamField", (IEnumerable<SelectListItem>)ViewBag.menu, "All", new { @class = "form-control" })*@
                                        @Html.TextBox("pivchSupplierName", null, new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    @Html.Label("Address", new { @class = "col-md-4 control-label" })
                                    <div class="col-lg-5">
                                        @*@Html.DropDownList("ParamField", (IEnumerable<SelectListItem>)ViewBag.menu, "All", new { @class = "form-control" })*@
                                        @Html.TextArea("pivchAddress", null, new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    @Html.Label("City", new { @class = "col-md-4 control-label" })
                                    <div class="col-lg-5">
                                        @*@Html.DropDownList("ParamField", (IEnumerable<SelectListItem>)ViewBag.menu, "All", new { @class = "form-control" })*@
                                        @Html.TextBox("pivchCity", null, new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    @Html.Label("Zip Code", new { @class = "col-md-4 control-label" })
                                    <div class="col-lg-5">
                                        @*@Html.DropDownList("ParamField", (IEnumerable<SelectListItem>)ViewBag.menu, "All", new { @class = "form-control" })*@
                                        @Html.TextBox("pivchZipCode", null, new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    @Html.Label("Phone", new { @class = "col-md-4 control-label" })
                                    <div class="col-lg-5">
                                        @*@Html.DropDownList("ParamField", (IEnumerable<SelectListItem>)ViewBag.menu, "All", new { @class = "form-control" })*@
                                        @Html.TextBox("pivchPhone", null, new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    @Html.Label("NPWP", new { @class = "col-md-4 control-label" })
                                    <div class="col-lg-5">
                                        @*@Html.DropDownList("ParamField", (IEnumerable<SelectListItem>)ViewBag.menu, "All", new { @class = "form-control" })*@
                                        @Html.TextBox("pivchNPWP", null, new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    @Html.Label("Active State", new { @class = "col-md-4 control-label" })
                                    <div class="col-lg-5">
                                        @Html.DropDownList("activeState", (SelectList)ViewBag.IsActive, "All", new { @class = "form-control" })
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            @*<input type="submit" value="Proses" id="buttonSearch" class="btn btn-default" />
                <input type="button" id="buttonCreate" value="Back" name="buttonAdd" class="btn btn-default" onclick="window.location.href('@Url.Action("Users","Users")')" />*@
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <input type="submit" value="Proses" id="buttonSearch" class="btn btn-default" />
                            <input type="button" id="buttonCreate" value="Back" name="buttonAdd" class="btn btn-default" onclick="window.location.href='@Url.Action("ProblemType", "Master", new { id = ViewBag.menuId })'" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}
