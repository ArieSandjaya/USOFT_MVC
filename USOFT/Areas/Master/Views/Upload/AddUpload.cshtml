@{
    ViewBag.Title = "insertUpload";
}
&nbsp;
@using (Html.BeginForm(FormMethod.Post))
{
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h5 class="text-white">New Measurement</h5>
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-8">
                            <form role="form">
                                <div class="form-group">
                                    @Html.Label("Upload Id", new { @class = "col-md-4 control-label" })
                                    <div class="col-lg-5">
                                        @Html.TextBox("SearchValue", null, new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    @Html.Label("Directory Path Name", new { @class = "col-md-4 control-label" })
                                    <div class="col-lg-5">
                                        @Html.TextBox("DirPath", null, new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    @Html.Label("File Ext", new { @class = "col-md-4 control-label" })
                                    <div class="col-lg-5">
                                        @Html.TextBox("FileExt", null, new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    @Html.Label("File Size", new { @class = "col-md-4 control-label" })
                                    <div class="col-lg-5">
                                        @Html.TextBox("FileSize", null, new { @class = "form-control" })
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <button type="Submit" value="Proses" class="btn btn-default btn-sm" id="buttonSearch">
                                <i class="glyphicon glyphicon-floppy-save"></i> Process
                            </button>
                            <button type="button" value="Back" class="btn btn-default btn-sm" id="buttonCreate" name="buttonAdd" onclick="window.location.href='@Url.Action("Upload", "Upload", new { id = ViewBag.menuId })'">
                                <i class="glyphicon glyphicon-backward"></i> Back
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}