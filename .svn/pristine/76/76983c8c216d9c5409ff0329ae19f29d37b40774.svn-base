
@{
    ViewBag.Title = "AddITItemIn";
}


@using (@Html.BeginForm(FormMethod.Post))
{
        <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">ITEM RECEIVE</h1>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Add New Receive Item
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-8">

                            <form role="form">
                                <div class="form-group">
                                    @Html.Label("Item in Code", new { @class = "col-md-4 control-label" })

                                    <div class="col-lg-5">
                                        @*@Html.DropDownList("ParamField", (IEnumerable<SelectListItem>)ViewBag.menu, "All", new { @class = "form-control" })*@
                                        @Html.Label("[Generated By System]", new { @class = "col-lg-10 control-label" })
                                    </div>

                                </div>
                                <div class="form-group">
                                    @Html.Label("Date In :", new { @class = "col-md-4 control-label" })
                                    <div class="col-lg-5">
                                        @Html.TextBox("datepicker", null, new { @class = "form-control" })
                                        @* @Html.DropDownList("BranchId", (SelectList)ViewBag.br, "All", new { @class = "form-control" })*@
                                    </div>
                                </div>
                                <div class="form-group">
                                    @Html.Label("Receive From", new { @class = "col-md-4 control-label" })
                                    <div class="col-lg-5">
                                        @Html.DropDownList("Receive", (SelectList)ViewBag.Receive, "All", new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="form-group" style="display:none" id="vendor">
                                    @Html.Label("Vendor", new { @class = "col-md-4 control-label" })
                                    <div class="col-lg-5">
                                        @Html.DropDownList("pivchSupplierCode", (SelectList)ViewBag.sup, "All", new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="form-group" style="display:none" id="branch">
                                    @Html.Label("Branch", new { @class = "col-md-4 control-label" })
                                    <div class="col-lg-5">
                                        @Html.DropDownList("piintBranchFrom", (SelectList)ViewBag.br, "All", new { @class = "form-control" })
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            @*<input type="submit" value="Proses" id="buttonSearch" class="btn btn-default" />
                <input type="button" id="buttonCreate" value="Back" name="buttonAdd" class="btn btn-default" onclick="window.location.href('@Url.Action("Users","Users")')" />*@
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <input type="submit" value="Proses" id="buttonSearch" class="btn btn-default" />
                            @if ((ViewData["Privilege"] as List<USOFT.DataAccess.spGetUserInfo_Result>)[0].CanInsert != false)
                            {
                            <input type="button" id="buttonCreate" value="Back" name="buttonAdd" class="btn btn-default" onclick="window.location.href='@Url.Action("Departement","Departement",new {id = ViewBag.menuId})'" />
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
@*<div id="container">
    <div class="div_common">
        @Html.Label("Item In Code :")
        @Html.Label("[ Generated By System ]")
        <p>
            @Html.Label("Date In :")
            @Html.TextBox("datepicker", null, new { @class = "input", @name = "pidtmDateIn" })
        </p>
        @Html.Label("Receive From")
       @Html.DropDownList("Receive", (SelectList)ViewBag.Receive, "All", new { @class = "combobox" })
        <div id="branch" style="display:none">
            @Html.DropDownList("piintBranchFrom", (IEnumerable<SelectListItem>)ViewBag.br, "- Select One -", new { @class = "combobox" })
        </div>
        <div id="vendor" style="display:none">
            @Html.DropDownList("pivchSupplierCode", (IEnumerable<SelectListItem>)ViewBag.sup, "- Select One -", new { @class = "combobox" })
        </div>
    </div>
</div>
<p><input type="Submit" value="Proses" class="buttonSub" id="buttonSearch" name="command" /></p>*@
}
<script type="text/javascript">

       


    $("#Receive").change(function () {
        var targID = $(this).val();
        $("#branch").hide();
        $("#vendor").hide();
        if (targID == "Vendor") {

            $('#vendor').show();
        } else
            if (targID == "Branch") {
                $('#branch').show();
                
            }



    })

</script>